            {
                "id": "person",
                "field": "person",
                "type": "radio",
                "question": "今日吃貨有幾位？",
                "options": [
                    {
                        "id": "single",
                        "label": "一人"
                    },
                    {
                        "id": "double",
                        "label": "雙人"
                    },
                    {
                        "id": "group",
                        "label": "多人"
                    }
                ]
            },
            {
                "id": "diet",
                "field": "diet",
                "type": "radio",
                "question": "飲食限制？",
                "options": [
                    {
                        "id": "none",
                        "label": "不限"
                    },
                    {
                        "id": "vegetarian",
                        "label": "素食"
                    }
                ]
            },
            {
                "id": "rating",
                "field": "rating",
                "type": "radio",
                "question": "預算範圍",
                "options": [
                    {
                        "id": "budget",
                        "label": "平價"
                    },
                    {
                        "id": "medium",
                        "label": "中等"
                    },
                    {
                        "id": "premium",
                        "label": "高價"
                    }
                ]
            },
            {
                "id": "preference",
                "field": "preference",
                "type": "checkbox",
                "question": "飲食偏好(複選)",
                "options": [
                    {
                        "id": "thai",
                        "label": "泰式"
                    },
                    {
                        "id": "japanese",
                        "label": "日式"
                    },
                    {
                        "id": "korean",
                        "label": "韓式"
                    },
                    {
                        "id": "italian",
                        "label": "義式"
                    },
                    {
                        "id": "chinese",
                        "label": "中式"
                    }
                ]
            },
            {
                "id": "occasion",
                "field": "occasion",
                "type": "checkbox",
                "question": "用餐場合(複選)",
                "options": [
                    {
                        "id": "casual",
                        "label": "日常"
                    },
                    {
                        "id": "celebrate",
                        "label": "慶祝"
                    },
                    {
                        "id": "romantic",
                        "label": "約會"
                    },
                    {
                        "id": "family",
                        "label": "家人"
                    }
                ]
            }
        ],
        "required": {
            "radio": [
                "person",
                "diet",
                "rating"
            ],
            "checkbox": [
                "preference",
                "occasion"
            ]
        }
